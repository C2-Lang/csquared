function printFibonnaci(numbers: int) void {
	let a, b, next = 0, 1, 0;
	
	for (i = 0; i < numbers; i++) {
		if (i == 0) {
			printf("%d\n", a);
		} else if (i == 1) {
			printf("%d\n", a);
		} else {
			next = a + b;
			printf("%d\n", next);
			a, b = b, next; // 'a = b', then 'b = next'
		}
	}
}

function main() int {
	printFibonnaci(numbers: 10);
	return 0;
}
